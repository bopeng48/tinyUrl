 <% if(!logged_in) { %>
<form action="/login" method="POST" style="margin:150px;">
<input type="text" name="username" placeholder="log in" style="width: 300px">
<input type="password" name="password" placeholder="password" style="width: 300px">
<input type="submit" value="Submit">
</form>
<% }%>


<% if(logged_in) { %>
  <p>  <%= user_id%> </p>
<form action="/logout/<%=user_id%>" method="POST" style="margin:150px;">
 <input type="submit" value="logout">
 </form>
<% } %>


<a href= "/register">register</a>

 <a href="/login">login</a>
